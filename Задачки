
#include <iostream>
using namespace std;

// Задача 1
int factorial(int n)
{
    int f = 1;
    for (int i = 1; i <= n; i++)
    {
        f *= i;
    }
    return f;
}

// Задача 2
// Извините, я такое не знаю, прям вообще не знаю

int main()
{
    int n;
    cin >> n;

    cout << factorial(n) << endl;
    system("pause");
}
